<!DOCTYPE html>
<html lang="en">

<head>
  <!-- Global site tag (gtag.js) - Google Analytics -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=G-TLCCPKXGNG"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag() { dataLayer.push(arguments); }
    gtag('js', new Date());

    gtag('config', 'G-TLCCPKXGNG');
  </script>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="description" content="Trang tìm kiếm link tải Fshare nhanh chóng, chính xác">
  <meta name="keywords"
    content="tai fshare, timfshare, tim link tai fshare, tim kiem link fshare, tim link tai phim, tim link tai">
  <title>
    <%= title %>
  </title>

  <link rel="shortcut icon" href="/favicon.png" type="image/x-icon">

  <link href="//fonts.googleapis.com/css?family=Quicksand:400,700" rel="stylesheet">
  <link href="/css/bootstrap.css" rel="stylesheet" />
  <link rel="stylesheet" href="/stylesheets/loader.css">
  <link rel='stylesheet' href="/stylesheets/style.css">
  <link rel='stylesheet' href="/stylesheets/autoacomplete.css">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
</head>

<body>
  <div class="container-fluid">
    <div class="row">
      <div class="col-12" id="logoout">
        <a href="/"><img src="/images/cse.png" alt="" class="logo top-0 start-50 translate-middle d-none d-sm-block"
            id="logo-outside"></a>
        <a href="/"><img src="/images/cse.png" alt="" class="mlogo top-0 start-50 translate-middle d-block d-sm-none"
            id="logo-outside"></a>
      </div>
    </div>
    <div class="row">
      <div class="col-10" id="inputsearchout">
        <div class="wrap-search start-10">
          <form action="/" method="get">
            <!-- <div class=""><img src="/images/search.png" type="submit" alt="Submit" class="search" id="img-search"></div> -->
            <div class="dropdown" id="cusSelectbox">
              <select name="site" class="dropdown-select" id="cusSelectboxValue">
                <option value="">Tất cả</option>
                <option value="fshare">Fshare</option>
                <option value="hdvietnam">HDvietnam</option>
                <option value="thuvienhd">ThuvienHD</option>
              </select>
            </div>
            <input autocomplete="off" type="text" placeholder="Nhập vào từ khóa..." id="input-search" name="s">
            <button type="submit" id="icon-search" style="border: unset !important;"> <i class="fas fa-search"></i>
            </button>
          </form>
        </div>

      </div>
      <div class="col-12">
        <div class="top-detail">
          <a href="/?top=key" class="title " id="searchkey">Top Từ Khóa</a> | <a href="/?top=link" class="title"
            id="searchkey">Top Đường Link Hot</a>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-xl-2">
        <a href="/"><img src="/images/cse.png" alt="" class="logoin start-10 d-none d-sm-block" id="logo-outside"></a>
        <a href="/"><img src="/images/cse.png" alt="" class="mlogoin start-25 logoin d-block d-sm-none"
            id="logo-outside"></a>
      </div>
      <div class="col-xl-10">
        <div class="wrap-search-in">
          <form action="/" method="get">
            <div class=""><img src="/images/search.png" type="submit" alt="Submit" class="search" id="img-search"></div>
            <div class="dropdown" id="cusSelectbox">
              <select name="site" class="dropdown-select" id="cusSelectboxValue">
                <option value="">Tất cả</option>
                <option value="fshare">Fshare</option>
                <option value="hdvietnam">HDvietnam</option>
                <option value="thuvienhd">ThuvienHD</option>
              </select>
            </div>
            <input autocomplete="off" type="text" placeholder="Nhập vào từ khóa..." id="input-search-in" name="s">
            <button type="submit" id="icon-search" style="border: unset !important;" > <i class="fas fa-search"></i>
            </button>
          </form>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-xl-2">
      </div>
      <div class="col-xl-5">
        <div class="wrap-recommendlink-page">
          <ul id="recommendlinkpage"></ul>
        </div>
      </div>
      <div class="col-xl-5">

        <div class="wrap-recommendkey-page">
          <ul id="recommendkeypage"></ul>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-xl-2">
      </div>
      <div class="col-xl-7">
        <div class="wrap-content">
          <ul id="results" />
        </div>
        <div class="page" id="pagination">
          <button class="prev" id="btn-prev">Trang trước</button>
          <button class="next" id="btn-next">Trang kế</button>
        </div>
      </div>

      <div class="col-xl-3" id="recommend">
        <div class="wrap-recommendlink">
          <ul id="recommendlink"></ul>
        </div>
        <div class="wrap-recommendkey">
          <ul id="recommendkey"></ul>
        </div>
      </div>
    </div>



    <div id="loader" style="color: #eb1a58" class="la-ball-climbing-dot la-3x loader">
      <div></div>
      <div></div>
      <div></div>
      <div></div>
    </div>
    <div id="input-searchautocomplete-list" class="autocomplete-items"></div>
    <img src="/images/cse.png" alt="Fshare Search" class="cse" id="img-cse">
    <script src="/javascripts/autocomplete.js"></script>
    <span style="display: none;" id="key_value">
      <%= JSON.stringify(keysearch) %>
    </span>
    <script src="/javascripts/main.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script>
      $(document).ready(function () {
        var key = $('#key_value').html();
        //key = unescape(key);
        key = JSON.parse(key);
        autocomplete(document.getElementById("input-search"), key);
        autocomplete(document.getElementById("input-search-in"), key);
        if (currentPage) {
          currentPage = 'home';
        }
      });
    </script>
  </div>
</body>

</html>